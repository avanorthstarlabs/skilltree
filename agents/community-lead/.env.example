# SkillForge Community Lead — Environment Variables
# Copy this to .env and fill in real values.

# Neynar (Farcaster API)
# Get your API key at https://dev.neynar.com
NEYNAR_API_KEY=your-neynar-api-key
NEYNAR_SIGNER_UUID=your-signer-uuid

# ElevenLabs (Voice synthesis)
# Get your API key at https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_VOICE_ID=your-voice-id

# Anthropic (Claude — the brain)
ANTHROPIC_API_KEY=your-anthropic-api-key

# SkillForge marketplace URL (for fetching live data)
SKILLFORGE_URL=http://localhost:3001

# Agent identity
AGENT_FID=0
AGENT_USERNAME=skillforge
AGENT_DISPLAY_NAME=SkillForge

# Posting schedule (cron expressions)
CRON_DAILY_UPDATE="0 10 * * *"
CRON_ENGAGEMENT_CHECK="*/15 * * * *"
